<!-- Portrait Pipeline -->
<Pipeline name="PORTRAIT">
   <CommandQueue>
      <Stage id="Init">
         <SwitchTarget target="" />
         <ClearTarget colBuf0="true" depthBuf="true"/>
      </Stage>
      
      <Stage id="Depth">
         <DrawGeometry context="DEPTH" forceLodLevel="-2"/>
      </Stage>

      <Stage id="Light">
         <DoForwardLightLoop suffix="PORTRAIT"/>
      </Stage>

      <Stage id="Emissive">
         <DrawGeometry context="EMISSIVE" forceLodLevel="0"/>
      </Stage>
	</CommandQueue>
</Pipeline>