   },

   "server_init_script" : "file(autotest_framework_server)",
   "client_init_script" : "file(autotest_framework_client)",
   
   "functions" : {
      "fail": {
         "controller" : "file(call_handlers/autotest_server_call_handlers.lua)",
         "endpoint" : "server"
